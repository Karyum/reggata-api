{
    "name": "lpad-api",
    "version": "1.0.0",
    "description": "",
    "main": "index.js",
    "scripts": {
        "dev": "NODE_ENV=development ts-node-dev src/server.ts",
        "build": "tsc",
        "start": "node dist/server.js",
        "postinstall": "tsc",
        "test": "NODE_ENV=test jest --detectOpenHandles",
        "reset:db": "npm run rollback:db && npm run migrate:lts:db && npm run seed:db",
        "rollback:db": "knex migrate:rollback --all --knexfile ./src/database/knexfile",
        "seed:db": "knex seed:run --knexfile ./src/database/knexfile",
        "migrate:lts:db": "ts-node ./node_modules/.bin/knex migrate:latest --knexfile ./src/database/knexfile",
        "create:migration": "knex migrate:make --knexfile ./src/database/knexfile",
        "create:seed": "knex seed:make --knexfile ./src/database/knexfile"
    },
    "author": "",
    "license": "ISC",
    "devDependencies": {
        "@types/node": "^17.0.5",
        "@types/node-cron": "^3.0.1",
        "babel-eslint": "^10.1.0",
        "eslint": "^8.14.0",
        "eslint-config-prettier": "^8.5.0",
        "prettier": "^2.6.2",
        "ts-node-dev": "^1.1.8",
        "tslib": "^2.6.2"
    },
    "dependencies": {
        "@types/express": "^4.17.13",
        "axios": "^0.27.2",
        "bcrypt": "^5.0.1",
        "compression": "^1.7.4",
        "connect-session-knex": "^2.1.1",
        "cors": "^2.8.5",
        "dotenv": "^10.0.0",
        "express": "^4.17.2",
        "express-rate-limit": "^6.0.1",
        "express-session": "^1.17.2",
        "helmet": "^4.6.0",
        "http-status": "^1.5.0",
        "joi": "^17.5.0",
        "jsonwebtoken": "^9.0.0",
        "knex": "^0.95.15",
        "module-alias": "^2.2.2",
        "moment": "^2.29.3",
        "node-cron": "^3.0.0",
        "pg": "^8.7.1",
        "postcss": "^8.4.12",
        "socket.io": "^4.6.0",
        "socket.io-client": "^4.6.0",
        "ts-node": "^10.9.1",
        "typescript": "^4.6.4",
        "winston": "^3.3.3"
    },
    "browserslist": [
        "Safari >=4"
    ],
    "packageManager": "npm@7.13.0",
    "engines": {
        "npm": ">=7.0.0",
        "node": ">=15.0.0"
    }
}
